{
    "name": "nonamephp/php7-common",
    "description": "A collection of common PHP 7 libraries",
    "version": "0.3.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Clint Tyler",
            "email": "ctyler@cdtwebsolutions.com"
        }
    ],
    "keywords": ["noname", "nonamephp", "common", "collection", "validator"],
    "homepage": "https://github.com/nonamephp/php7-common",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Noname\\Common\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Noname\\Common\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
